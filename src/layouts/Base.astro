---
const { title = "Vital Fit Peptides" } = Astro.props;
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <title>{title}</title>
  </head>
  <body>
    <header style="padding:12px;border-bottom:1px solid #ddd">
      <strong>Vital Fit Peptides Header</strong>
    </header>

    <main style="padding:16px">
      <slot />
    </main>

    <footer style="padding:12px;border-top:1px solid #ddd">
      Footer
    </footer>
  </body>
</html>